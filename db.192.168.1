;
; BIND data file for local loopback interface
;
$ORIGIN 101.168.192.in-addr.arpa.
$TTL	604800
@	IN	SOA	admin root@correos.practica.net (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	IN	NS	admin.practicadaw.net.
101	IN	PTR	pc1.practicadaw.net.
102	IN	PTR	pc2.practicadaw.net.
103	IN	PTR	pc3.practicadaw.net.
